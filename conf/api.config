listenaddr: 0.0.0.0:8888
debugmode: true
mysqldsn: api:api@tcp(127.0.0.1:3306)/api?charset=utf8mb4&parseTime=True&loc=Local
proxyhandlerconfig:
  pacgeneratecron: 0 0 * * *
  pacfile: false
  pacfilepath: ""
systemdhandlerconfig:
  monitorservicenames:
  - ssh.service
  - kibana.service
  - elasticsearch.service
  - filebeat.service
  - metricbeat.service
  - node_exporter.service
  - grafana.service
  - prometheus.service
